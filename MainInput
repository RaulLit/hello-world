import java.util.*;
import AIConvo;

public class MainClass {
  public static void main(String [] args) {
  Scanner sc = new Scanner(System.in);
  AIConvo obj = new AIConvo();
  boolean convo = true;
  while(convo) {
  String userInput = sc.nextLine();
  System.out.println(obj.botConvo());
  if(userInput == "bye") {
    System.out.println(obj.botConvo());
    convo = false;
  }
}
